
/*node class of the linked list
class Node
{
    int data;
    Node next;
    Node(int key)
    {
        data = key;
        next = null;
    }
}
*/
class GfG
{
   
    public static int getNth(Node node, int ind)
    {
       //Your code here
       Node cur =node;
       for(int i=0;i<ind-1;i++){
           cur =cur.next;
       }
       return cur.data;
    }
}
